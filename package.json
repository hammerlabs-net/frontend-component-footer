{
  "name": "@edx/frontend-component-footer",
  "version": "1.0.0-alpha",
  "description": "Footer component for use when building Open edX frontend applications",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "pilet"
  ],
  "scripts": {
    "build": "pilet build",
    "start": "pilet debug",
    "upgrade": "pilet upgrade",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "test": "fedx-scripts jest --coverage"
  },
  "source": "./src/index.jsx",
  "piral": {
    "name": "frontend-app-shell"
  },
  "files": [
    "/dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openedx/frontend-component-footer.git"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/openedx/frontend-component-footer/issues"
  },
  "homepage": "https://github.com/openedx/frontend-component-footer#readme",
  "devDependencies": {
    "@edx/frontend-build": "npm:@hammerlabs-net/frontend-build@1.0.0-alpha",
    "@edx/reactifex": "^2.1.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.7",
    "frontend-app-shell": "file:../frontend-app-shell/dist/emulator/frontend-app-shell-1.0.0.tgz",
    "husky": "8.0.3",
    "piral-cli": "1.0.1",
    "piral-cli-webpack5": "1.0.1",
    "react-redux": "8.1.1",
    "react-test-renderer": "16.14.0",
    "redux": "4.2.1",
    "semantic-release": "21.0.5",
    "tslib": "2.3.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "prop-types": "15.8.1"
  },
  "peerDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@edx/frontend-platform": "npm:@hammerlabs-net/frontend-platform@1.0.0-alpha",
    "@edx/paragon": "npm:@hammerlabs-net/paragon@1.0.0-alpha",
    "@edx/brand": "npm:@edx/brand-openedx@1.2.0",
    "react": "^16.14.0 || ^17.0.0",
    "react-dom": "^16.14.0 || ^17.0.0",
    "react-router-dom": "5.3.4"
  }
}
